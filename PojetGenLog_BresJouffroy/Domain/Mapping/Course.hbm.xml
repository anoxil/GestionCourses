<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Course" table="course">

    <id name="IdCourse" column="idCourse" type="int">
      <generator class="native"/>
    </id>
    <property name="TypeCourse" column="course_type" not-null="true"/>
    <property name="Nom" column="course_nom" not-null="true"/>
    <property name="NbKm" column="course_km" not-null="true"/>

  </class>

</hibernate-mapping>